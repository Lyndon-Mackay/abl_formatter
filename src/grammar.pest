string = ${double_quote_string| single_quote_string}

double_quote_string = _{ "\"" ~ inner ~ "\"" ~ (":"~ ASCII_ALPHA)?}
inner = @{ char* }
char = {
    !("\"" | "\\") ~ ANY
    | "\\" ~ ("\"" | "\\" | "/" | "b" | "f" | "n" | "r" | "t")
    | "\\" ~ ("u" ~ ASCII_HEX_DIGIT{4})
}

single_quote_string = ${ "'" ~single_quote_inner ~ "'"}

single_quote_inner = @{single_quote_char*}

single_quote_char = {
    !("'" | "\\") ~ ANY
    | "\\" ~ ("\"" | "\\" | "/" | "b" | "f" | "n" | "r" | "t")
    | "\\" ~ ("u" ~ ASCII_HEX_DIGIT{4})
}

COMMENT =@{"/*" ~ comment_content ~ ("*/"| (COMMENT ~ comment_content)* ~ "*/") }
comment_content = { (!("*/"|"/*") ~ ANY) * }

WHITESPACE = {tab_space |NEWLINE }
NEWLINE = {"\r\n" | "\n"}

tab_space = _{ " "| "\t" }

date = {ASCII_DIGIT{2}~"/"~ASCII_DIGIT{2}~"/"~ASCII_DIGIT{2}}

decimal = {ASCII_DIGIT{1,40} ~ "." ~ASCII_DIGIT{1,10} }

big_int = {"-"? ~ ASCII_DIGIT{1,19}}
int = {"-"? ~ ASCII_DIGIT{1,10}}
logical = {^"TRUE"|^"FALSE"|^"YES"|^"NO"}

datatype = ${int | big_int | logical | string | date }

operator = {"+" | "-" | "*" | "/" | "=" | "<" | "<=" | ">" | ">=" | ^"EQ"| ^"NE" | ^"GT" | ^"GE" | ^"LT" | ^"LE" | ^"NOT" | ^"AND" | ^"OR" | "^MATCHES" }


subvar = _{ ("_"| "$"| (ASCII_ALPHANUMERIC))+ ~  ("-"| "$"| "_" |"/"| (ASCII_ALPHANUMERIC))* }

variable = ${ !(operator ~ WHITESPACE | keyword ~ WHITESPACE) ~ subvar ~ ("." ~ subvar)?  } // used for capturing variables

block_begin = { ":"}

loop_label = @{ variable~ ":"}

block_end = { ^"END" ~ ^"procedure" ~ "." | ^"END" ~ "."}

statement_end = { "." }

left_parenthesis = {"("}

right_parenthesis = {")"}

comma = {","}
//distinguished from regular brackets by a space
function = ${
    (keyword | variable) ~ left_parenthesis  ~ (function_content)* ~ right_parenthesis
}

function_content = { !")" ~ ( WHITESPACE | COMMENT | keyword | expression | comma ) }

/* This is more general then the actual language but not verifying syntax 
exmaple 5 NOT 2 makes no sense in the abl progress language but for formatting purposes is ok */
expression = {
    datatype ~ (operator ~ expression )?  |
    function ~ (operator ~ expression)? |
    (!^"WHEN" ~ keyword) ~ left_parenthesis ~ expression ~ right_parenthesis ~ (operator ~ expression)? |
    variable ~ left_parenthesis ~ expression ~ right_parenthesis ~ (operator ~ expression)? |
    variable ~( operator ~ expression)?   |
    left_parenthesis ~ expression ~ right_parenthesis ~ (operator ~ expression)?

}

conditional_expression_pred = {
    (^"IF" | ^"WHERE" | ^"WHILE")
}
//expressions starting with if or where will lielly be formatted differentky in future.
conditional_expression = {
    conditional_expression_pred   ~ expression
}

field_keyword = { ^"FIELD"}

aslike_keyword = { ^"AS" | ^"LIKE" }

like_keyword = {^"LIKE"}

define_keyword = {^"DEFINE"}

temptable_keyword = {^"TEMP-TABLE"}

temp_table_single_field = {
    field_keyword ~ variable ~ aslike_keyword ~ keyword ~ (!(^"FIELD"|statement_end) ~ (keyword | ANY) )* 
}
temp_table_fields = { temp_table_single_field* }

temp_table_like = {
    like_keyword ~ variable 
}

define_temp_table = {
    (define_keyword ~ (!temptable_keyword ~ keyword)? ~ temptable_keyword ~ variable ~ (!(^"FIELD"|^"LIKE") ~ keyword)* ) ~
    (temp_table_like)? ~
    temp_table_fields ~ (!field_keyword ~ keyword )*

}

assign_keyword = {^"ASSIGN"}

input_keyword = {^"INPUT"}

framebrowse_keyword = {^"FRAME" | ^"BROWSE"}

when_keyword = {^"WHEN"}
noerror_keyword = {^"NO-ERROR"}

equals = { "="}

assign_line = { variable ~ equals ~ expression ~  (when_keyword ~ expression)? }

assign_lines = {assign_line+}

assign_statement = {
    assign_keyword ~ (input_keyword? ~ framebrowse_keyword ~ variable )?
    ~ assign_lines
    ~ noerror_keyword?
}

include = ${ "{" ~ inner_include ~ ("}"| (include ~ inner_include)* ~ "}" )  }
inner_include = { (!("}"|"{") ~ ANY)*}

statement = { 
    loop_label | block_end | 
    (( define_temp_table | assign_statement | conditional_expression | keyword | comma | expression |  datatype)* 
    ~ ( block_begin |  statement_end ))
}

program = { (NEWLINE | include | statement | COMMENT  )* ~ EOI}

